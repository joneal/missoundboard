{
	"Version": "2012-10-17",
	"Statement": [	
		{
			"Sid": "GetAnduinInstallers",
			"Effect": "Allow",
			"Principal": {
				"AWS": "arn:aws:iam::359476562016:user/anduin-installer"
			},
			"Action": "s3:GetObject",
			"Resource": "arn:aws:s3:::anduin-installers/*"
		},
		{
			"Sid": "EditManifest",
			"Effect": "Allow",
			"Principal": {
				"AWS": "arn:aws:iam::359476562016:user/anduin-installer"
			},
			"Action": [
				"s3:PutObject",
				"s3:PutObjectAcl"
			],
			"Resource": "arn:aws:s3:::anduin-installers/manifest.json"
		}
	]
}